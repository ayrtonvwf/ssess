{
    "name": "ayrtonvwf/ssess",
    "description": "A security-first PHP session handler",
    "type": "library",
    "version": "0.0.1-beta",
    "require": {
        "ext-openssl": "*",
        "ext-json": "*",
        "php": ">=7.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ayrton Fidelis",
            "email": "ayrton.vargas33@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ssess\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "mikey179/vfsStream": "^1.6",
        "phpstan/phpstan": "^0.10.3",
        "phpmd/phpmd": "^2.6"
    },
    "scripts": {
        "build": "composer test && composer phpstan && composer phpmd",
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "phpmd": "phpmd src text cleancode,codesize,controversial,design,naming,unusedcode"
    }
}
